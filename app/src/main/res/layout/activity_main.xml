<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline"
            app:layout_constraintGuide_percent="0.3"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline2"
            app:layout_constraintGuide_percent="0.25"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline3"
            app:layout_constraintGuide_percent="0.5"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline4"
            app:layout_constraintGuide_percent="0.75"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline5"
            app:layout_constraintGuide_percent="0.4514364"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline6"
            app:layout_constraintGuide_percent="0.6019152"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline7"
            app:layout_constraintGuide_percent="0.75"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline8"
            app:layout_constraintGuide_percent="0.9"/>
    <Button
            android:text="4"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button4"
            app:layout_constraintEnd_toStartOf="@+id/guideline2" app:layout_constraintTop_toTopOf="@+id/guideline6"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toTopOf="@+id/guideline7"
            android:textSize="30sp"/>
    <Button
            android:text="5"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button5"
            app:layout_constraintTop_toTopOf="@+id/guideline6"
            app:layout_constraintBottom_toTopOf="@+id/guideline7" app:layout_constraintEnd_toStartOf="@+id/guideline3"
            app:layout_constraintStart_toStartOf="@+id/guideline2" android:textSize="30sp"/>
    <Button
            android:text="6"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button6"
            app:layout_constraintTop_toTopOf="@+id/guideline6"
            app:layout_constraintBottom_toTopOf="@+id/guideline7"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintEnd_toStartOf="@+id/guideline4" android:textSize="30sp"/>
    <Button
            android:text="7"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button7"
            app:layout_constraintTop_toTopOf="@+id/guideline5"
            app:layout_constraintBottom_toTopOf="@+id/guideline6" app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintStart_toStartOf="parent" android:textSize="30sp"/>
    <Button
            android:text="8"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button8"
            app:layout_constraintTop_toTopOf="@+id/guideline5"
            app:layout_constraintBottom_toTopOf="@+id/guideline6"
            app:layout_constraintStart_toStartOf="@+id/guideline2"
            app:layout_constraintEnd_toStartOf="@+id/guideline3" app:layout_constraintHorizontal_bias="0.605"
            app:layout_constraintVertical_bias="0.487" android:textSize="30sp"/>
    <Button
            android:text="9"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button9"
            app:layout_constraintTop_toTopOf="@+id/guideline5"
            app:layout_constraintBottom_toTopOf="@+id/guideline6" app:layout_constraintEnd_toStartOf="@+id/guideline4"
            app:layout_constraintStart_toStartOf="@+id/guideline3" android:textSize="30sp"/>
    <Button
            android:text="z"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button11"
            app:layout_constraintTop_toTopOf="@+id/guideline"
            app:layout_constraintBottom_toTopOf="@+id/guideline5" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/guideline2" android:textSize="30sp" android:background="#03A9F4"/>
    <Button
            android:text="y"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button12"
            app:layout_constraintStart_toStartOf="@+id/guideline2"
            app:layout_constraintEnd_toStartOf="@+id/guideline3" app:layout_constraintTop_toTopOf="@+id/guideline"
            app:layout_constraintBottom_toTopOf="@+id/guideline5" android:textSize="30sp" android:background="#03A9F4"/>
    <Button
            android:text="z"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button13"
            app:layout_constraintTop_toTopOf="@+id/guideline" app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintEnd_toStartOf="@+id/guideline4" app:layout_constraintBottom_toTopOf="@+id/guideline5"
            android:textSize="30sp" android:background="#03A9F4"/>
    <Button
            android:text="1"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button1"
            app:layout_constraintTop_toTopOf="@+id/guideline7"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toTopOf="@+id/guideline8"
            android:textSize="30sp"/>
    <Button
            android:text="2"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button2"
            app:layout_constraintTop_toTopOf="@+id/guideline7" app:layout_constraintEnd_toStartOf="@+id/guideline3"
            app:layout_constraintStart_toStartOf="@+id/guideline2"
            app:layout_constraintBottom_toTopOf="@+id/guideline8" android:textSize="30sp"/>
    <Button
            android:text="3"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button3"
            app:layout_constraintTop_toTopOf="@+id/guideline7" app:layout_constraintEnd_toStartOf="@+id/guideline4"
            app:layout_constraintBottom_toTopOf="@+id/guideline8"
            app:layout_constraintStart_toStartOf="@+id/guideline3" android:textSize="30sp"/>
    <Button
            android:text="*"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button17"
            app:layout_constraintTop_toTopOf="@+id/guideline5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/guideline6"
            app:layout_constraintStart_toStartOf="@+id/guideline4" app:layout_constraintHorizontal_bias="0.478"
            app:layout_constraintVertical_bias="0.513" android:textSize="30sp" android:background="#FF5722"
            android:layout_marginBottom="1dp"/>
    <Button
            android:text="/"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button18"
            app:layout_constraintTop_toTopOf="@+id/guideline"
            app:layout_constraintBottom_toTopOf="@+id/guideline5"
            app:layout_constraintStart_toStartOf="@+id/guideline4" app:layout_constraintEnd_toEndOf="parent"
            android:textSize="30sp" android:background="#FF5722" android:layout_marginBottom="1dp"/>
    <Button
            android:text="-"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button19"
            app:layout_constraintTop_toTopOf="@+id/guideline6" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/guideline7"
            app:layout_constraintStart_toStartOf="@+id/guideline4" android:textSize="30sp"
            android:background="#FF5722" android:layout_marginBottom="1dp"/>
    <Button
            android:text="+"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button20"
            app:layout_constraintTop_toTopOf="@+id/guideline7" app:layout_constraintStart_toStartOf="@+id/guideline4"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/guideline8"
            android:textSize="30sp" android:background="#FF5722" android:layout_marginBottom="1dp"/>
    <Button
            android:text="0"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button10"
            app:layout_constraintEnd_toStartOf="@+id/guideline3" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline8" app:layout_constraintBottom_toBottomOf="parent"
            android:textSize="30sp"/>
    <Button
            android:text="."
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button22"
            app:layout_constraintTop_toTopOf="@+id/guideline8"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toStartOf="@+id/guideline4"
            app:layout_constraintStart_toStartOf="@+id/guideline3" android:textSize="30sp"/>
    <Button
            android:text="="
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button23"
            app:layout_constraintTop_toTopOf="@+id/guideline8" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="@+id/guideline4"
            android:textSize="30sp" android:background="#FF5722"/>
    <TextView
            android:text="0"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toTopOf="@+id/guideline"
            android:gravity="right|center_horizontal|center_vertical" android:textSize="36sp"
            android:layout_marginRight="16dp" android:layout_marginEnd="16dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>